#include <iostream>
using namespace std;

int main()
{
    int n, g,h,s;
    cin >> n >> g >> h;
    int** a = new int* [n];
    for (int i = 0; i < n; i++)
    {
        a[i] = new int[g];
    }
    for (int i = 0; i < n; i++)
    {
        for (int z = 0; z < g; z++)
        {
            cin >> a[i][z];
        }
    }
    for (int i = 0; i < n;i++)
    {
        s = 0;
        for (int j = 0; j < g; j++)
        {
            s += a[i][j];
        }
        if (s <= h)
        {
            for (int x = i; x < n-1; x++)
            {
                for (int j = 0; j < g; j++)
                {
                    a[x][j] = a[x + 1][j];
                }
            }
            n--;
            i--;
        }
    }
    for (int i = 0; i < n; i++)
    {
        for (int z = 0; z < g; z++)
        {
            cout << a[i][z] << " ";
        }
        cout << endl;
    }
}
