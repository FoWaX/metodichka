#include <iostream>
using namespace std;

int main()
{
    int n = 10;
    int a[10];
    for (int i = 0; i < n; i++)
    {
        a[i] = rand() % 10;
    }
    a[6] = 9;
    for (int i = 0; i < n; i++)
    {
        cout << a[i] << endl;
    }
    int f = 0;
    for (int i = 0; i < n; i++)
    {
        if (a[i] > f)
        {
            f = a[i];
        }
    }
    for (int i = 0; i < n; i++)
    {
        if (a[i] != f)
        {
            cout << i << " ";
        }
    }
}
