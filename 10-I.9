#include <iostream>
#include <cmath>
using namespace std;


struct point
{
	double x, y, z;
};

double dlina(point g, point h)
{
	return sqrt(pow((h.x - g.x), 2) + pow((h.y - g.y), 2) + pow((h.z - g.z), 2));
}
int main()
{
	point circle, a[100],max;
	double R;
	int n;
	int kmax = -100;
	int k;
	cin >> R >> n;
	for (int i = 0;i < n;i++)
	{
		cin >> a[i].x >> a[i].y >> a[i].z;
	}
	for (int i = 0;i < n;i++)
	{
		circle.x = a[i].x;
		circle.y = a[i].y;
		circle.z = a[i].z;
		k = 0;
		for (int v = 0;v < n;v++)
		{
			if (dlina(a[v], circle) <= R)
			{
				k++;
			}
		}
		if (k > kmax)
		{
			kmax = k;
			max.x = circle.x;
			max.y = circle.y;
			max.z = circle.z;
		}
	}
	cout << max.x << " " << max.y << " " << max.z << endl;
}
